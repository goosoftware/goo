{
  "name": "react-typescript",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "electron-esbuild dev",
    "build": "electron-esbuild build",
    "package": "electron-builder"
  },
  "devDependencies": {
    "@solana/web3.js": "^1.15.0",
    "@types/node": "~15.6.1",
    "@types/react": "~17.0.8",
    "@types/react-dom": "~17.0.5",
    "electron": "^13.0.0",
    "electron-builder": "^22.10.5",
    "electron-esbuild": "^1.4.1",
    "electron-util": "~0.16.0",
    "esbuild": "^0.10.2",
    "react": "~17.0.2",
    "react-dom": "~17.0.2",
    "typescript": "^4.3.2"
  },
  "build": {
    "appId": "io.comp.myapp",
    "productName": "Graph Editor",
    "extraMetadata": {
      "name": "Graph Editor",
      "main": "main.js"
    },
    "files": [
      {
        "from": ".",
        "filter": [
          "package.json"
        ]
      },
      {
        "from": "dist/main"
      },
      {
        "from": "dist/renderer"
      }
    ],
    "win": {
      "target": [
        "zip"
      ]
    },
    "mac": {
      "target": [
        "zip"
      ]
    },
    "linux": {
      "target": [
        "zip"
      ]
    },
    "directories": {
      "buildResources": "resources"
    },
    "publish": null
  },
  "dependencies": {
    "litegraph.js": "^0.7.9",
    "tcp-port-used": "^1.0.2"
  }
}
