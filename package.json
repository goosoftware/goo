{
  "name": "goo",
  "version": "0.0.1",
  "license": "MIT",
  "private": true,
  "description": "a visual development environment for solana",
  "homepage": "https://goo.software",
  "repository": {
    "url": "https://github.com/goosoftware/goo"
  },
  "scripts": {
    "dev": "electron-esbuild dev",
    "build": "electron-esbuild build",
    "package": "electron-builder"
  },
  "dependencies": {
    "@headlessui/react": "^1.2.0",
    "@heroicons/react": "^1.0.1",
    "@solana/web3.js": "^1.17.0",
    "camelcase": "^6.2.0",
    "glob": "^7.1.7",
    "json-circular-stringify": "^1.0.1",
    "litegraph.js": "^0.7.9",
    "mobx": "^6.3.2",
    "mobx-react-lite": "^3.2.0",
    "mobx-state-tree": "^5.0.1",
    "natsort": "^2.0.2",
    "react": "~17.0.2",
    "react-dom": "~17.0.2",
    "react-icons": "^4.2.0",
    "react-jazzicon": "^0.1.3",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-toastify": "^7.0.4"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.3.3",
    "@types/ed2curve": "^0.2.2",
    "@types/jest": "^26.0.23",
    "@types/node": "~15.6.2",
    "@types/react": "~17.0.9",
    "@types/react-dom": "~17.0.6",
    "@types/react-router-dom": "^5.1.7",
    "autoprefixer": "^10.2.6",
    "electron": "^13.1.1",
    "electron-builder": "^22.10.5",
    "electron-esbuild": "^1.5.0",
    "electron-util": "~0.16.0",
    "esbuild": "^0.10.2",
    "esbuild-jest": "^0.5.0",
    "esbuild-plugin-postcss2": "^0.0.9",
    "jest": "^27.0.4",
    "nightwind": "^1.1.11",
    "postcss": "^8.3.0",
    "postcss-modules": "4.1.0",
    "tailwindcss": "^2.1.4",
    "typescript": "^4.3.2"
  },
  "build": {
    "appId": "io.comp.myapp",
    "productName": "Graph Editor",
    "extraMetadata": {
      "name": "Graph Editor",
      "main": "main.js"
    },
    "files": [
      {
        "from": ".",
        "filter": [
          "package.json"
        ]
      },
      {
        "from": "dist/main"
      },
      {
        "from": "dist/renderer"
      }
    ],
    "win": {
      "target": [
        "zip"
      ]
    },
    "mac": {
      "target": [
        "zip"
      ]
    },
    "linux": {
      "target": [
        "zip"
      ]
    },
    "directories": {
      "buildResources": "resources"
    },
    "publish": null
  }
}
